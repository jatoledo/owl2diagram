        <rdfs:label xml:lang="en">distribution Channel Type</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="https://w3id.org/transmodel/fares#fareType">
        <rdfs:comment xml:lang="en">This property is used to link a fare with the types of fares available in the SKOS concept scheme of types of fares</rdfs:comment>
        <rdfs:comment xml:lang="en">fare type</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="https://w3id.org/transmodel/fares#governedBy">
